{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "school-service",
            "image": "kpadania/microservice-docker-school-service",
            "hostname": "school-service",
            "essential": false,
            "memory": 128
        },
        {
            "name": "school-ui",
            "image": "kpadania/microservice-docker-school-ui",
            "hostname": "school-ui",
            "essential": false,
            "memory": 128
        },
        {
            "name": "config",
            "image": "kpadania/microservice-docker-config",
            "hostname": "config",
            "essential": false,
            "memory": 128
        },
        {
            "name": "discovery",
            "image": "kpadania/microservice-docker-discovery",
            "hostname": "discovery",
            "essential": false,
            "memory": 128
        }
    ]
}